{% extends 'base.html.twig' %}

{% block title %}Audiometrie index{% endblock %}

{% block body %}
    <h1>Audiometrie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Designation</th>
                <th>Typeaudiometrie</th>
                <th>Typeoreille</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for audiometrie in audiometries %}
            <tr>
                <td>{{ audiometrie.id }}</td>
                <td>{{ audiometrie.designation }}</td>
                <td>{{ audiometrie.typeaudiometrie }}</td>
                <td>{{ audiometrie.typeoreille }}</td>
                <td>
                    <a href="{{ path('app_audiometrie_show', {'id': audiometrie.id}) }}">show</a>
                    <a href="{{ path('app_audiometrie_edit', {'id': audiometrie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_audiometrie_new') }}">Create new</a>
{% endblock %}
